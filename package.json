{
  "name": "handlatex",
  "version": "1.0.0",
  "description": "Realtime handwriting conversion to LaTeX.",
  "main": "index.js",
"scripts": {
  "start": "concurrently \"npm run start-frontend\" \"npm run start-backend\"",
  "start-frontend": "cd frontend && cross-env NODE_OPTIONS=--openssl-legacy-provider npm start",
  "start-backend": "cd backend && ./.venv/bin/python app.py",
  "build": "cd frontend && npm run build",
  "test": "cd frontend && npm run test",
  "eject": "cd frontend && npm run eject"
},

  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3"
  }
}
